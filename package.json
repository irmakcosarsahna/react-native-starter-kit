{
  "name": "reactnativestarterkit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "initial-load": "npx react-native-rename ${0} && yarn && yarn pods",
    "pre-commit": "npx lint-staged && yarn test",
    "start": "react-native start --reset-cache",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "lint": "eslint --ext .js,jsx .",
    "lint:fix": "yarn run lint --fix",
    "clear": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules && npm cache clean --force && rm -rf package-lock.json && rm -rf ios/build && rm -rf android/app/build && rm -rf ios/Podfile.lock && rm -rf ios/Pods",
    "pods": "cd ios && pod install && cd ..",
    "pods-update": "cd ios && pod install --repo-update && cd ..",
    "fix": "yarn fix-webview",
    "fix-webview": "cp module_modifications/react-native-webview/apple/RNCWebView.m node_modules/react-native-webview/apple/RNCWebView.m",
    "android-apk": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "android-release": "npx react-native run-android --variant=release",
    "fresh": "yarn clear && yarn && yarn pods && npx jetifier",
    "prettify": "yarn run lint:fix; prettier --write '**/*.{ts,tsx}' && prettier --write '*.{ts,tsx}'",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.8",
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^6.0.2",
    "@react-navigation/bottom-tabs": "^6.0.7",
    "@react-navigation/native": "^6.0.4",
    "@react-navigation/stack": "^6.0.9",
    "axios": "^0.22.0",
    "formik": "^2.2.9",
    "i18n-js": "^3.8.0",
    "i18next": "^21.2.3",
    "intl": "^1.2.5",
    "lodash": "^4.17.21",
    "lottie-ios": "^3.2.3",
    "lottie-react-native": "^4.1.3",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-i18next": "^11.12.0",
    "react-native": "^0.66.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^2.1.5",
    "react-native-masked-text": "^1.13.0",
    "react-native-modal-datetime-picker": "^11.0.0",
    "react-native-reanimated": "2.3.0-beta.2",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.1",
    "react-native-vector-icons": "^8.1.0",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "validator": "^13.6.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-native": "^0.65.0",
    "@types/react-redux": "^7.1.18",
    "@types/react-test-renderer": "^16.9.2",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "^18.20.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest-cli": "^27.2.0",
    "lint-staged": "^11.1.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "mocha": "^9.1.1",
    "react-test-renderer": "17.0.2",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "verbose": false,
    "transformIgnorePatterns": [
      "node_modules/?!(react-navigation)"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  }
}
